{
  "manifest_version": 3,
  "name": "Gitlab Merge Request Tools",
  "version": "0.1.0",
  "description": "Gitlab Merge Request Tools",
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["thirdParty/jquery-3.5.1.min.js", "content.js"],
    "matches": ["https://gitlab.com/*/merge_requests/*"]
  }],
  "icons": {
    "16": "image/icon-16x16.png",
    "32": "image/icon-32x32.png",
    "96": "image/icon-96x96.png"
  }
}
